<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kwk.pos.view.TableOverviewController">
   <children>
      <GridPane fx:id="tableOverviewGrid" prefHeight="800.0" prefWidth="1200.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="990.0" minWidth="10.0" prefWidth="97.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="990.0" minWidth="0.0" prefWidth="69.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="675.0" minWidth="10.0" prefWidth="363.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="685.0" minWidth="10.0" prefWidth="32.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="677.0" minWidth="10.0" prefWidth="642.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="382.0" minHeight="10.0" prefHeight="68.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="704.0" minHeight="0.0" prefHeight="703.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="689.0" minHeight="10.0" prefHeight="87.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="previousButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="&lt; Back" />
            <Text fx:id="tableNum" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="89.0" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
            <TableView fx:id="tableOrderTable" prefHeight="295.0" prefWidth="573.0" GridPane.columnSpan="3" GridPane.rowIndex="1">
              <columns>
                  <TableColumn fx:id="productNumberColumn" prefWidth="39.0" text="No." />
                <TableColumn fx:id="productNameColumn" maxWidth="555.0" minWidth="-Infinity" prefWidth="196.0" text="Product Name" />
                <TableColumn fx:id="productPriceColumn" prefWidth="98.0" text="Product Price" />
                  <TableColumn fx:id="productQuantityColumn" prefWidth="100.0" text="Quantity" />
                  <TableColumn fx:id="productSumColumn" prefWidth="92.0" text="Sum" />
              </columns>
            </TableView>
            <ButtonBar GridPane.columnIndex="2" GridPane.rowIndex="2">
               <buttons>
                  <Button fx:id="addItemButton" mnemonicParsing="false" onAction="#handleAddItem" text="Add Item" />
                  <Button fx:id="deleteItemButton" mnemonicParsing="false" onAction="#handleDeleteItem" text="Delete Item" />
               </buttons>
            </ButtonBar>
            <SubScene fx:id="tableSubscene" height="349.0" width="642.0" GridPane.columnIndex="4" GridPane.rowIndex="1">
              <root>
                <Region />
              </root>
            </SubScene>
         </children>
      </GridPane>
   </children>
</AnchorPane>
