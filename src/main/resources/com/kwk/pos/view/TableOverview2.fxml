            <TableColumn fx:id="orderQuantityColumn" editable="false" maxWidth="-Infinity" minWidth="60.0" prefWidth="75.0" resizable="false" sortable="false" text="Quantity" />
            <TableColumn fx:id="orderSumColumn" editable="false" maxWidth="-Infinity" minWidth="80.0" prefWidth="75.0" resizable="false" sortable="false" text="Sum" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
